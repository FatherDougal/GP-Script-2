{
  "name": "prova",
  "script_path": "/opt/.gentooplayer/web/scripts/alsasw0.sh",
  "description": "**Switching alsa version**  \n . \n **Select Alsa Version** ([**Info Alsa Version on Gentoo**](https://packages.gentoo.org/packages/media-libs/alsa-lib)) \n . \n .",
  "working_directory": "/tmp",
  "parameters": [
    {
      "name": "Server file",
      "param": "--server_file",
      "type": "server_file",
      "secure": true,
      "file_dir": "/var/log",
      "file_extensions": [
        "log"
      ]
    },
    {
      "name": "open file iso",
      "param": "-c",
      "type": "server_file",
      "required": true,
      "secure": true,
      "file_dir": "..",
      "file_recursive": true,
      "file_extensions": [
        "iso"
      ],
      "description": "List alsa version"
    },
    {
      "name": "open file iso1",
      "param": "-d",
      "type": "server_file",
      "required": true,
      "secure": true,
      "file_recursive": true,
      "description": "List alsa version"
    }
  ]
}
